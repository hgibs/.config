<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
         "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.hollandgibson.tldr-update</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/tldr</string>
        <string>--update</string>
    </array>
    <key>StandardErrorPath</key>
    <string>/Users/hollandgibson/com.hollandgibson.task.err</string>
    <key>StandardOutPath</key>
    <string>/Users/hollandgibson/com.hollandgibson.task.out</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>0</integer>
        <key>Minute</key>
        <integer>3</integer>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>Nice</key>
    <integer>15</integer>
</dict>
</plist>
